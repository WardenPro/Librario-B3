{
  "name": "node",
  "packageManager": "yarn@4.5.0",
  "devDependencies": {
        "@types/cookie-parser": "^1",
        "@types/cors": "^2",
        "@types/csurf": "^1",
        "@types/express": "^5.0.0",
        "@types/node": "^22.7.6",
        "@types/swagger-jsdoc": "^6",
        "@types/swagger-ui-express": "^4.1.7",
        "drizzle-kit": "^0.26.2",
        "eslint": "^9.12.0",
        "prettier": "3.3.3",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.10.0"
  },
  "scripts": {
    "c": "yarn tsc",
    "start": "node dist/",
    "all": "yarn c && yarn start",
    "static": "eslint .",
    "stylecheck": "prettier --check '**/*.{js,jsx,ts,tsx,css,scss,json}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,json}'",
    "clear": "rm -rf dist",
    "auto": "concurrently \"yarn tsc --watch\" \"nodemon dist/index.js\"",
    "dev": "nodemon --watch src --ext ts --exec 'yarn all'"
  },
  "dependencies": {
    "drizzle-orm": "^0.35.2",
    "drizzle-zod": "^0.5.1",
    "zod": "^3.23.8"
  }
}
